{
  "name": "sveltemplate",
  "version": "1.0.0",
  "author": "Kota Yatagai <kota@yatagai.com>",
  "keywords": [
    "kotayatagai",
    "kota-yata",
    "yatagai",
    "八谷",
    "航太",
    "八谷航太",
    "ヤタガイコウタ"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --single",
    "format": "run-s format:eslint format:prettier format:stylelint",
    "format:eslint": "eslint --fix",
    "format:prettier": "prettier --write --svelte-sort-order scripts-markup-styles 'src/**/*.svelte'",
    "format:stylelint": "stylelint --fix '**/*.{css,scss,sass}'"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.26.10",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.1.1"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@rollup/plugin-replace": "^2.3.3",
    "dotenv": "^8.2.0",
    "firebase": "^7.19.1",
    "firebase-admin": "^9.1.1",
    "firebase-functions": "^3.11.0",
    "ityped": "^1.0.3",
    "sirv-cli": "^1.0.0",
    "svelte-awesome": "^2.3.0",
    "svelte-i18n": "^3.0.4",
    "svelte-swiper": "^1.0.1",
    "svelte-toast": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,svelte}": [
      "prettier --write --svelte-sort-order scripts-markup-styles 'src/**/*.svelte'"
    ],
    "*.scss": [
      "stylelint --fix '**/*.{css,scss,sass}'"
    ]
  }
}
